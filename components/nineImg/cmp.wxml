
<view class='container'>
  <view class='img-box'>
    <image 
    wx:for="{{imgArr}}" wx:key="{{index}}"
    wx:if="{{index < 9}}"
    src="{{item}}"
    data-index="{{index}}"
    catchtap='onTap'>
    </image>

    <text class='img-num' wx:if="{{imgArr.length > 9}}">{{imgArr.length}}</text>   
  </view>
  <view class='text-box'>{{mainTitle}}</view>
</view>

